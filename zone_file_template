/$TTL $MTTL
@       IN      SOA     $DOMAIN  $MAIL (
                                    $SERIAL;
                                    $REFRESH;
                                    $RETRY;
                                    $EXPIRY;
                                    $MTTL);
;
@       IN      NS      $NS
$NS			IN      A       $IP
$WWW    IN      CNAME   $IPWWW
$EXTRA1
$EXTRA2
$EXTRA3

